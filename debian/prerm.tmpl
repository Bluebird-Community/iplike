#!/bin/sh
# prerm script for iplike-pgsql
#
# see: dh_installdeb(1)

set -e

POSTGRESQL_VERSION="@POSTGRESQL_VERSION@"
PG_TYPE_VERSION="@PG_TYPE_VERSION@"
PG_PLUGINDIR="@PG_PLUGINDIR@"

case "$1" in
    remove)
        update-alternatives --remove "iplike.so"         "${PG_PLUGINDIR}/iplike.so"
        update-alternatives --remove "install_iplike.sh" "/usr/sbin/install_iplike-${PG_TYPE_VERSION}.sh"
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0


